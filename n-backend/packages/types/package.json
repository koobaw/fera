{
  "name": "@cainz-next-gen/types",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
